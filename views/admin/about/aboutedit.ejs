<%- include ('../../header.ejs') %>

    <section class="dashboard-wrap mtb-40">
        <div class="container">
            <div class="body-content">
                <div class="row">
                    <div class="col-md-3"></div>
                    <div class="col-md-9">
                        <div class="dash-right">
                            <div class="dash-header">
                                <div class="dash-title">
                                    <h1>About Edit </h1>
                                </div>
                                
                            </div>
                            <form method="post" action="/aboutupdate/<%= aboutedit._id %>" enctype="multipart/form-data" >
                                
                                
                               
                                <div class="form-group form-row">
                                    <label class="col-md-3 required">Description *</label>
                                    <div class="col-md-9">
                                        <textarea name="description" rows="5"  class="form-control" placeholder="Description"
                                          required> <%= aboutedit.description %> </textarea>
                                    </div>
                                </div>
                                
    
                                <div class="form-group form-row">
                                    <label class="col-md-3"></label>
                                    <div class="col-md-9">
                                        <button type="submit" class="btn btn-primary">Update</button>
                                    </div>
                                </div>
    
    
    
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        
        
    </section>

    <%- include ('../../footer.ejs') %>